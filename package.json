{
  "name": "kpm-docs",
  "version": "1.2.3",
  "description": "The TeamKUNPluginManager Documentation",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build:page": "docusaurus build",
    "build:javadoc": "ts-node ./scripts/build-javadoc.ts",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "format": "prettier --write --cache .",
    "check:format": "prettier --cache --check ."
  },
  "homepage": "https://github.com/TeamKUN/KPMDocs",
  "dependencies": {
    "@docusaurus/core": "2.2.0",
    "@docusaurus/preset-classic": "2.2.0",
    "@docusaurus/theme-common": "^2.2.0",
    "@docusaurus/plugin-client-redirects": "^2.2.0",
    "@fec/remark-a11y-emoji": "^3.1.0",
    "@fontsource/jetbrains-mono": "^4.5.11",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@mdx-js/react": "^1.6.22",
    "docusaurus-plugin-image-zoom": "^0.1.1",
    "budoux": "^0.4.0",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.2.0",
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "lint-staged": "^13.0.3",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "simple-git": "^3.16.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix",
        "prettier --write"
      ]
    },
    "ignore": [
      "build/**/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
